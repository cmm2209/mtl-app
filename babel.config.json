{
    "presets": [
        "react",
        "es2015",
        ["@babel/preset-react"],
        [
            "@babel/preset-env",
            {
                "targets": {
                    "browsers": "last 2 versions"
                },
                "loose": true,
                "modules": false
            }
        ]
    ],
    "plugins": ["@babel/syntax-dynamic-import"]
}
